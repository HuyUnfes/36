local espEnabled = false
local foundMirage = false
local foundMirageObject = nil

local function sendNotification(title, text)
    pcall(function()
        StarterGui:SetCore("SendNotification", {
            Title = title;
            Text = text;
            Duration = 10;
        })
    end)
end

-- Hàm lấy part đại diện để gắn ESP
local function getRepresentativePart(object)
    if not object then return nil end
    if object:IsA("BasePart") then return object end
    if object:IsA("Model") then
        if object.PrimaryPart then return object.PrimaryPart end
        -- Lấy part to nhất trong model để làm tâm
        local biggestPart = nil
        local maxMass = 0
        for _, v in pairs(object:GetDescendants()) do
            if v:IsA("BasePart") then
                local mass = v:GetMass()
                if mass > maxMass then
                    maxMass = mass
                    biggestPart = v
                end
            end
        end
        return biggestPart or object:FindFirstChildWhichIsA("BasePart", true)
    end
    return nil
end

local function createESP(object)
    if not object then return end
    local targetPart = getRepresentativePart(object)
    if not targetPart then return end

    if targetPart:FindFirstChild("MirageESP_Strict") then return end

    local bgui = Instance.new("BillboardGui")
    bgui.Name = "MirageESP_Strict"
    bgui.Adornee = targetPart
    bgui.Size = UDim2.new(0, 200, 0, 50)
    bgui.StudsOffset = Vector3.new(0, 150, 0)
    bgui.AlwaysOnTop = true
    bgui.Parent = targetPart

    local label = Instance.new("TextLabel")
    label.Parent = bgui
    label.BackgroundTransparency = 1
    label.Size = UDim2.new(1, 0, 1, 0)
    label.TextColor3 = Color3.fromRGB(0, 255, 127) -- Màu Spring Green
    label.TextStrokeTransparency = 0
    label.TextStrokeColor3 = Color3.fromRGB(0,0,0)
    label.TextSize = 18
    label.Font = Enum.Font.GothamBlack
    
    local updateLoop
    updateLoop = RunService.RenderStepped:Connect(function()
        if not espEnabled or not targetPart or not targetPart.Parent or not bgui then
            if updateLoop then updateLoop:Disconnect() end
            if bgui then bgui:Destroy() end
            return
        end
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local dist = math.floor((LocalPlayer.Character.HumanoidRootPart.Position - targetPart.Position).Magnitude)
            label.Text = "REAL MIRAGE (ĐẢO THẬT)\n[" .. dist .. "m]"
        end
    end)
    
    -- Gửi thông báo xác nhận
    local dist = math.floor((LocalPlayer.Character.HumanoidRootPart.Position - targetPart.Position).Magnitude)
    sendNotification("TÌM THẤY HÀNG THẬT!", "Khoảng cách: " .. dist .. "m")
end

-- [[ LOGIC QUÉT NGHIÊM NGẶT ]] --
local function scanForMirage()
    -- Chỉ tìm trong Workspace (Mirage luôn spawn ở Workspace hoặc Workspace.Map)
    -- KHÔNG dùng GetDescendants toàn bộ game để tránh lag và tránh UI
    
    local candidates = {}
    
    -- 1. Lấy danh sách các object tiềm năng từ Workspace
    for _, v in pairs(Workspace:GetChildren()) do
        table.insert(candidates, v)
    end
    
    -- 2. Lấy danh sách từ Map (nếu có)
    local mapFolder = Workspace:FindFirstChild("Map")
    if mapFolder then
        for _, v in pairs(mapFolder:GetChildren()) do
            table.insert(candidates, v)
        end
    end

    -- 3. Duyệt danh sách với điều kiện khắt khe
    for _, v in pairs(candidates) do
        local name = v.Name
        
        -- ĐIỀU KIỆN 1: Tên phải CHÍNH XÁC là "Mirage Island" (Blox Fruits đặt tên này)
        -- Hoặc tên chứa "Mirage" nhưng KHÔNG ĐƯỢC chứa "Script", "UI", "Sound"
        if (name == "Mirage Island" or (name:find("Mirage") and name:find("Island"))) then
            
            -- ĐIỀU KIỆN 2: Phải là Model hoặc Folder (Đảo không thể là 1 Part nhỏ)
            if v:IsA("Model") or v:IsA("Folder") then
                
                -- ĐIỀU KIỆN 3: Kiểm tra kích thước (Đảo thật phải rất to)
                -- Chúng ta tìm một MeshPart bất kỳ bên trong để đo kích thước
                local hugePart = false
                for _, part in pairs(v:GetDescendants()) do
                    if part:IsA("BasePart") and part.Size.Magnitude > 500 then
                        hugePart = true
                        break
                    end
                end
                
                -- ĐIỀU KIỆN 4: (Optional) Tìm NPC Advanced Fruit Dealer
                -- NPC này chỉ spawn trên Mirage
                local hasDealer = v:FindFirstChild("AdvancedFruitDealer", true) or v:FindFirstChild("Advanced Fruit Dealer", true)

                if hugePart or hasDealer then
                    return v
                end
            end
        end
    end

    return nil
end

local function toggleESP()
    espEnabled = not espEnabled
    if not espEnabled then
        foundMirage = false
        foundMirageObject = nil
        for _, v in pairs(Workspace:GetDescendants()) do
            if v.Name == "MirageESP_Strict" then v:Destroy() end
        end
    end
    return espEnabled
end

local function extractJobId(text)
    if not text then return nil end
    local pattern = "%x%x%x%x%x%x%x%x%-%x%x%x%x%-%x%x%x%x%-%x%x%x%x%-%x%x%x%x%x%x%x%x%x%x%x%x"
    return string.match(text, pattern)
end

